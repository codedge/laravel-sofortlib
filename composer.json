{
    "name": "codedge/laravel-sofortlib",
    "description" : "Laravel 5 package for use with SofortLib/SOFORT API, provides a facade to work with.",
    "license": "MIT",
    "keywords": [
          "sofortlib",
          "laravel",
          "sofortüberweisung",
          "sofortueberweisung",
          "paycode",
          "sofort api"
    ],
    "authors": [
        {
            "name": "Holger Lösken",
            "email": "holger.loesken@codedge.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "sofort/sofortlib-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Codedge\\Sofortlib\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
